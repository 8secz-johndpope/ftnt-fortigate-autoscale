
config router static
    edit 1
        set dst 168.63.129.16 255.255.255.255
        set gateway 10.0.1.1
        set priority 5
        set device "port2"
    next
    edit 2
        set dst 168.63.129.16 255.255.255.255
        set gateway 10.0.2.1
        set priority 5
        set device "port3"
    next
    edit 3
        set dst 168.63.129.16 255.255.255.255
        set gateway 10.0.3.1
        set priority 5
        set device "port4"
    next
    edit 4
        set dst 168.63.129.16 255.255.255.255
        set gateway 10.0.0.1
        set device "port1"
    next
end
